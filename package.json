{
  "name": "synapse",
  "version": "1.0.0",
  "description": "Synapse - Universal Intelligent Learning & Coding Assistant Platform",
  "private": true,
  "scripts": {
    "dev": "pnpm -r run dev",
    "build": "pnpm -r run build",
    "test": "pnpm -r run test",
    "lint": "pnpm -r run lint",
    "clean": "pnpm -r run clean",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "setup": "pnpm install && pnpm docker:up",
    "db:migrate": "pnpm --filter @synapse/database run db:migrate",
    "db:seed": "pnpm --filter @synapse/database run db:seed"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "pnpm": "^8.15.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0"
}

