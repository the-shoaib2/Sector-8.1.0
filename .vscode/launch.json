{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Extension",
      "type": "extensionHost",
      "request": "launch",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/synapse-dev-extension"
      ],
      "outFiles": [
        "${workspaceFolder}/synapse-dev-extension/dist/**/*.js"
      ],
      "preLaunchTask": "npm: compile"
    }
  ]
}
